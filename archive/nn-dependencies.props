# output TXT format
outputFormat = text

# input file location
file = ../data/raw/selected_samples.txt

# output file location
<!-- outputDirectory = ../data/parses/nn-dependencies-stanford/ -->
outputDirectory = ../data/parses/nn-dependencies/

# list of annotators to run
annotators = tokenize,ssplit,pos,depparse
# note - only dependency parse works for the shift-reduce parser

# customize the pos model
depparse.model = edu/stanford/nlp/models/parser/nndep/english_UD.gz
<!-- depparse.model = edu/stanford/nlp/models/parser/nndep/english_SD.gz -->

# Output Stanford dependencies (Universal dependencies otherwise)
parse.originalDependencies = false

parse.buildgraphs = true

# Enforce binary trees of the dependency parse (default=false)
parse.binarytrees = false
